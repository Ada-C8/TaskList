<!-- <h1>TaskList#new</h1> -->

<h1>add a task!</h1>

<%= form_for @task do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name %>

  <%= f.label :completion_date %>
  <!-- <%= f.text_field :completion_date, data:{ provide:'datepicker' } %> -->
  <!-- <input type="date" name="myDate" min="2013-06-01" max="2013-08-31" id="myDate"> -->
  <%= f.text_field :completion_date, type:"date", min: "2017-09-22", max: "2018-12-31" %>

  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.label :action %>
  <%= f.text_field :action%>

  <%= f.submit "save task"%>
<% end %>

