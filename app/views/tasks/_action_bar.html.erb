<% tasks.each do |task| %>
  <li>
    <strong><%= link_to "#{task[:title]}", task_path(task[:id]) %></strong><br/>Mark Complete | <%= link_to "edit", edit_task_path(task[:id]) %> |
    <%= link_to 'Delete', task_path(task), method: :delete, data: { confirm: 'Are you sure you want to delete this?' } %>
  </li>
<% end%>
